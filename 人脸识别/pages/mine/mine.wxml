<view class="bg">
        <view class="icon">
                <open-data type="userAvatarUrl"></open-data>
        </view>
        <open-data type="userNickName"></open-data>
</view>
<view class="system">
        <text class="info">设备信息</text>
        <view>
                <text>型号</text>
                <text>{{system.model}}</text>
        </view>
         <view>
                <text>版本</text>
                <text>{{system.SDKVersion}}</text>
        </view>
         <view>
                <text>分辨率</text>
                <text>{{system.screenWidth}}*{{system.screenHeight}}</text>
        </view>
         <view>
                <text>地区</text>
                        
                <view class="city">
                        <open-data lang="zh_CN" type="userCountry"></open-data>
                        <open-data lang="zh_CN" type="userProvince"></open-data>
                        <open-data lang="zh_CN" type="userCity"></open-data>

                </view>
        </view>
</view>

<view class="setting" bindtap="nextpage" >
        <text>系统设置</text>
        <text>></text>
</view>